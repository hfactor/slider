{
  "name": "@boxslider/react",
  "version": "2.2.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/boxslider/slider/packages/react"
  },
  "author": "Phil Parsons",
  "license": "MIT",
  "keywords": [
    "boxslider",
    "react",
    "slider",
    "carousel",
    "cube",
    "3D"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "pnpm run clean && pnpm run esbuild && pnpm run types",
    "clean": "rm -rf dist && rm -f LICENSE",
    "esbuild": "../../scripts/build-node.js react,react-dom,@boxslider/slider",
    "prepack": "cp ../../LICENSE ./",
    "types": "tsc --project tsconfig.types.json --emitDeclarationOnly --outDir dist/types"
  },
  "dependencies": {
    "@boxslider/slider": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  }
}
