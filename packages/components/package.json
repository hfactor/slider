{
  "name": "@boxslider/components",
  "version": "2.3.9",
  "description": "Box slider Web components",
  "repository": {
    "type": "git",
    "url": "https://github.com/boxslider/slider/packages/components"
  },
  "author": "Phil Parsons",
  "license": "MIT",
  "keywords": [
    "boxslider",
    "slider",
    "carousel",
    "cube",
    "3D",
    "webcomponents"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./style/": "./src/style/"
  },
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "pnpm run clean && pnpm run esbuild && pnpm run types",
    "dev": "vite",
    "clean": "rm -rf dist && rm -f LICENSE",
    "esbuild": "../../scripts/build-node.js && ../../scripts/build-browser.js",
    "prepack": "cp ../../LICENSE ./",
    "types": "tsc --project tsconfig.types.json --emitDeclarationOnly --outDir dist/types"
  },
  "dependencies": {
    "@boxslider/slider": "workspace:*"
  }
}
